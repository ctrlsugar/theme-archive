<script>
$(document).ready(function() {
    $('#trigger').click(function() {
        $('#postcontainer').fadeOut();
        $('#pagination').fadeOut();
        $('.popup').fadeIn();
    });
    
    $('.popup .close').click(function() {
        $('.popup').fadeOut();
        $('#postcontainer').fadeIn();
        $('#pagination').fadeIn();
    });
});
</script>

