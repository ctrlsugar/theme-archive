<script>
function openSugar(sugarName) {
  var i;
  var x = document.getElementsByClassName("content");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";  
  }
  document.getElementById(sugarName).style.display = "block";  
}
</script>

<script>
function openCtrl(ctrlName) {
  var i;
  var x = document.getElementsByClassName("contain");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";  
  }
  document.getElementById(ctrlName).style.display = "block";  
}
</script>

