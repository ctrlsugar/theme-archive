        <script>
            tippy('a[title], .photoset-image', {
              theme: 'tooltippy',
              zIndex: 9999999999,
              arrow: tippy.roundArrow,
              maxWidth: 300,
              placement: 'auto',

              content(reference) {
                const title = reference.getAttribute('title');
                reference.removeAttribute('title');
                return title;
              },
            });
        </script>


 
