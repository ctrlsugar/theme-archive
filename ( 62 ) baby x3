<script>
    $(document).ready(function() {      
    $('#pop').click(function() {
        $('#pop').fadeOut();
        $('.close').fadeIn();
        $('.posts').fadeOut();
        $('#notes').fadeOut();
        $('#pagination').fadeOut();
        $('.popup').fadeIn();
        
    });
    
    $('.popup #close').click(function() {
        $('.popup').fadeOut();
        $('.posts').fadeIn();
        $('#notes').fadeIn();
        $('#pagination').fadeIn();
        $('#pop').fadeIn();
    });
 });  
</script>

